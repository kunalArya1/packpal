
generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User{
  id Int @id @default(autoincrement())
  fullName String
  email String
  password String 
  Role String @default("Customer")
  Address String
  countryCode String
  Phone String
  Preferance String
}